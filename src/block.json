{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "dss/jobbnorge",
	"version": "2.0.0",
	"title": "Jobbnorge",
	"category": "widgets",
	"icon": "people",
	"description": "List jobs at jobbnorge.no",
	"keywords": [
		"jobbnorge",
		"jobbnorge.no"
	],
	"supports": {
		"html": false
	},
	"attributes": {
		"columns": {
			"type": "number",
			"default": 3
		},
		"blockLayout": {
			"type": "string",
			"default": "list"
		},
		"employerID": {
			"type": "string",
			"default": ""
		},
		"noJobsMessage": {
			"type": "string",
			"default": ""
		},
		"orderBy": {
			"type": "string",
			"default": "Deadline"
		},
		"itemsToShow": {
			"type": "number",
			"default": 5
		},
		"displayEmployer": {
			"type": "boolean",
			"default": false
		},
		"displayExcerpt": {
			"type": "boolean",
			"default": true
		},
		"displayDeadline": {
			"type": "boolean",
			"default": false
		},
		"displayScope": {
			"type": "boolean",
			"default": false
		},
		"displayDuration": {
			"type": "boolean",
			"default": false
		},
		"displayDate": {
			"type": "boolean",
			"default": true
		},
		"excerptLength": {
			"type": "number",
			"default": 55
		}
	},
	"textdomain": "wp-jobbnorge-block",
	"editorScript": "file:init.js",
	"editorStyle": "file:editor.scss",
	"style": "file:style.scss"
}